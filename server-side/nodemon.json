{
    "watch": [
        "src",
        "router",
        "models",
        "variables",
        "variables",
        "index.ts",
        ".env"
    ],
    "ext": "js,ts,json",
    "ignore": [
        "src/logs/*",
        "src/**/*.{spec,test}.ts"
    ],
    "exec": "ts-node --transpile-only index.ts"
}